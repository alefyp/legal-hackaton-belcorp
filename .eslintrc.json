{
    "env": {
      "browser": true,
      "es6": true
    },
    "extends": ["airbnb",  "plugin:react/recommended"],
    "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 11,
      "sourceType": "module"
    },
    "plugins": [ "react"],
    "rules": {
      "linebreak-style":"warn",
      "react/jsx-closing-bracket-location": 0,
      "react/jsx-filename-extension": [1, {
        "extensions": [".js", ".jsx"]}
        ]
    }

  }
  